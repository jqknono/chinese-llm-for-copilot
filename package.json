{
    "name": "coding-plans-for-copilot",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "0.2.0",
    "publisher": "techfetch-dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/jqknono/chinese-llm-for-copilot.git"
    },
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "Machine Learning",
        "Other"
    ],
    "icon": "assets/icon.png",
    "main": "./out/extension.js",
    "contributes": {
        "languageModelChatProviders": [
            {
                "vendor": "zhipu-ai",
                "displayName": "%providers.zhipu.displayName%",
                "configuration": {
                    "type": "object",
                    "required": [
                        "apiKey"
                    ],
                    "properties": {
                        "apiKey": {
                            "type": "string",
                            "title": "API Key",
                            "description": "Zhipu API key",
                            "secret": true
                        }
                    }
                }
            },
            {
                "vendor": "kimi-ai",
                "displayName": "%providers.kimi.displayName%",
                "configuration": {
                    "type": "object",
                    "required": [
                        "apiKey"
                    ],
                    "properties": {
                        "apiKey": {
                            "type": "string",
                            "title": "API Key",
                            "description": "Kimi API key",
                            "secret": true
                        }
                    }
                }
            },
            {
                "vendor": "volcengine-ai",
                "displayName": "%providers.volcengine.displayName%",
                "configuration": {
                    "type": "object",
                    "required": [
                        "apiKey"
                    ],
                    "properties": {
                        "apiKey": {
                            "type": "string",
                            "title": "API Key",
                            "description": "Volcengine API key",
                            "secret": true
                        }
                    }
                }
            },
            {
                "vendor": "minimax-ai",
                "displayName": "%providers.minimax.displayName%",
                "configuration": {
                    "type": "object",
                    "required": [
                        "apiKey"
                    ],
                    "properties": {
                        "apiKey": {
                            "type": "string",
                            "title": "API Key",
                            "description": "Minimax API key",
                            "secret": true
                        }
                    }
                }
            },
            {
                "vendor": "aliyun-ai",
                "displayName": "%providers.aliyun.displayName%",
                "configuration": {
                    "type": "object",
                    "required": [
                        "apiKey"
                    ],
                    "properties": {
                        "apiKey": {
                            "type": "string",
                            "title": "API Key",
                            "description": "Aliyun DashScope API key",
                            "secret": true
                        }
                    }
                }
            }
        ],
        "commands": [
            {
                "command": "coding-plans.manage.zhipu",
                "title": "%commands.coding-plans.manage.zhipu.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.setApiKey.zhipu",
                "title": "%commands.coding-plans.setApiKey.zhipu.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.refreshModels.zhipu",
                "title": "%commands.coding-plans.refreshModels.zhipu.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.manage.kimi",
                "title": "%commands.coding-plans.manage.kimi.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.setApiKey.kimi",
                "title": "%commands.coding-plans.setApiKey.kimi.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.refreshModels.kimi",
                "title": "%commands.coding-plans.refreshModels.kimi.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.manage.volcengine",
                "title": "%commands.coding-plans.manage.volcengine.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.setApiKey.volcengine",
                "title": "%commands.coding-plans.setApiKey.volcengine.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.refreshModels.volcengine",
                "title": "%commands.coding-plans.refreshModels.volcengine.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.manage.minimax",
                "title": "%commands.coding-plans.manage.minimax.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.setApiKey.minimax",
                "title": "%commands.coding-plans.setApiKey.minimax.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.refreshModels.minimax",
                "title": "%commands.coding-plans.refreshModels.minimax.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.manage.aliyun",
                "title": "%commands.coding-plans.manage.aliyun.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.setApiKey.aliyun",
                "title": "%commands.coding-plans.setApiKey.aliyun.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.refreshModels.aliyun",
                "title": "%commands.coding-plans.refreshModels.aliyun.title%",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.generateCommitMessage",
                "title": "%commands.coding-plans.generateCommitMessage.title%",
                "icon": "$(sparkle)",
                "category": "%commands.category%"
            },
            {
                "command": "coding-plans.selectCommitMessageModel",
                "title": "%commands.coding-plans.selectCommitMessageModel.title%",
                "category": "%commands.category%"
            }
        ],
        "keybindings": [
            {
                "command": "coding-plans.generateCommitMessage",
                "key": "ctrl+shift+g ctrl+shift+m",
                "mac": "cmd+shift+g cmd+shift+m"
            }
        ],
        "menus": {
            "scm/title": [
                {
                    "command": "coding-plans.generateCommitMessage",
                    "group": "navigation",
                    "when": "scmProvider == git"
                }
            ]
        },
        "configuration": {
            "title": "%configuration.title%",
            "properties": {
                "coding-plans.commitMessage.language": {
                    "type": "string",
                    "default": "en",
                    "enum": ["en", "zh-cn"],
                    "enumDescriptions": [
                        "English",
                        "中文"
                    ],
                    "description": "%configuration.coding-plans.commitMessage.language.description%",
                    "order": 0
                },
                "coding-plans.commitMessage.modelVendor": {
                    "type": "string",
                    "default": "",
                    "description": "%configuration.coding-plans.commitMessage.modelVendor.description%",
                    "order": 1
                },
                "coding-plans.commitMessage.modelId": {
                    "type": "string",
                    "default": "",
                    "description": "%configuration.coding-plans.commitMessage.modelId.description%",
                    "order": 2
                },
                "coding-plans.region": {
                    "type": "boolean",
                    "default": true,
                    "description": "%configuration.coding-plans.region.description%",
                    "order": 3
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./ && npm run copy-i18n",
        "watch": "tsc -watch -p ./",
        "copy-i18n": "copyfiles -u 1 \"src/i18n/*.json\" out/",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "changelog:update": "node ./scripts/update-changelog.js",
        "package:vsix": "npm run compile && npx @vscode/vsce package",
        "publish:marketplace": "npm run compile && node ./scripts/publish-vsce.js",
        "pricing:fetch": "node ./scripts/fetch-provider-pricing.js"
    },
    "devDependencies": {
        "@types/node": "^20.14.9",
        "@types/vscode": "^1.95.0",
        "@typescript-eslint/eslint-plugin": "^7.13.1",
        "@typescript-eslint/parser": "^7.13.1",
        "copyfiles": "^2.4.1",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "axios": "^1.7.2"
    }
}
